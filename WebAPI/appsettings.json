{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "clients": {
    "spaclient": {
      "RedirectUri": "https://localhost:7016/oauth2/callback"
    }

  },
  "Providers": {
    "Google": {
      "Authority": "https://accounts.google.com",
      "Authorization_Endpoint": "https://accounts.google.com/o/oauth2/v2/auth",
