      "RedirectUri": "https://localhost:7216/oauth2/authorizationcallback",
      "ClientSecret": "xyz",
      "SupportS265ChallengeMethod": false
    },
    "Facebook": {
      "Authority": "https://www.facebook.com",
      "Authorization_Endpoint": "https://www.facebook.com/v17.0/dialog/oauth",
      "Token_Endpoint": "https://graph.facebook.com/v17.0/oauth/access_token",
      "ClientId": "abc",
      "RedirectUri": "https://localhost:7216/authorizationcallback",
      "ClientSecret": "1234",
      "SupportS265ChallengeMethod": false
    }
  }

